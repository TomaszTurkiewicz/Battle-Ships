<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminPanel">

    <TextView
        android:id="@+id/textViewAuthUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="@dimen/TextSize"
        android:layout_marginTop="@dimen/FieldSize"
        android:layout_marginLeft="@dimen/FieldSize"
        android:text="Users Auth: "/>

    <TextView
        android:id="@+id/textViewAuthUserCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/textViewAuthUser"
        app:layout_constraintLeft_toRightOf="@id/textViewAuthUser"
        android:textSize="@dimen/TextSize"
        android:text="so many"/>


</androidx.constraintlayout.widget.ConstraintLayout>